{
  "version": "1.0.0",
  "providers": {
    "openai": {
      "api_key": "${OPENAI_API_KEY}",
      "model": "gpt-4"
    },
    "google": {
      "api_key": "${GOOGLE_API_KEY}",
      "model": "gemini-pro"
    },
    "openrouter": {
      "api_key": "${OPENROUTER_API_KEY}",
      "model": "openrouter/auto"
    },
    "cli": {
      "command": "echo 'CLI provider not configured'"
    }
  },
  "security": {
    "max_job_bytes": 26214400,
    "payload_allowlist": [
      ".md",
      ".txt",
      ".json",
      ".patch",
      ".diff",
      ".png",
      ".jpg",
      ".svg",
      ".zip"
    ],
    "allow_absolute_paths": false
  },
  "timeouts": {
    "cli_seconds": 60
  }
}
